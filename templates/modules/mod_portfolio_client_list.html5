<?php $this->extend('block_unsearchable'); ?>

<?php $this->block('content'); ?>

  <?php if (empty($this->clients)): ?>
    <p class="empty"><?php echo $this->empty; ?></p>
  <?php else: ?>
    <?php foreach($this->clients as $clients) { echo '<div class="row">'.implode('', $clients).'</div>'; } ?>
    <?php echo $this->pagination; ?>
  <?php endif; ?>

<?php $this->endblock(); ?>
